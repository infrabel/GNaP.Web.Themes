    <div class="tab-content">
        <div id="buttons" class="tab-pane in active">
            <h3 class="header smaller">
                Basic Buttons
            </h3>

            <button class="btn btn-primary">Primary</button>
            <button class="btn btn-primary">
                <i class="icon-cog"></i>
                Primary
            </button>
            {{> page.buttons_basic-buttons}}

            <h3 class="header smaller">
                Form Example
            </h3>

            <button class="btn btn-primary">Save</button>
            <button class="btn btn-default">Cancel</button>
            {{> page.buttons_form-example}}

            <h3 class="header smaller">
                Tooltips (hover over these)
            </h3>

            <button class="btn btn-primary" data-rel="tooltip" title="This is a default tooltip!">Default tooltip</button>
            <button class="btn btn-primary" data-rel="tooltip" data-placement="left" title="This is a tooltip on the left!">Left tooltip</button>
            <button class="btn btn-primary" data-rel="tooltip" data-placement="right" title="This is a tooltip on the right!">Right tooltip</button>
            <button class="btn btn-primary" data-rel="tooltip" data-placement="top" title="This is a tooltip on top!">Top tooltip</button>
            <button class="btn btn-primary" data-rel="tooltip" data-placement="bottom" title="This is a tooltip at the bottom!">Bottom tooltip</button>
            {{> page.buttons_tooltips}}

            <h3 class="header smaller">
                Various Sizes
            </h3>

            <button class="btn btn-primary btn-minier">Tiny</button>
            <button class="btn btn-primary btn-xs">Mini</button>
            <button class="btn btn-primary btn-sm">Small</button>
            <button class="btn btn-primary">Default</button>
            <button class="btn btn-primary btn-lg">Large</button>
            {{> page.buttons_various-sizes}}

            <h3 class="header smaller">
                Disabled Buttons
            </h3>

            <button class="btn btn-primary disabled">Disabled Primary</button>
            <button class="btn btn-default disabled">Disabled Default</button>
            {{> page.buttons_disabled-buttons}}

            <h3 class="header smaller">
                Dropdown Button
            </h3>

            <div class="btn-group">
                <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle">
                    Action <i class="icon-angle-down icon-on-right"></i>
                </button>
                <ul class="dropdown-menu">
                    <li>
                        <a href="#">Action</a>
                    </li>
                    <li>
                        <a href="#">Another action</a>
                    </li>
                    <li>
                        <a href="#">Something else here</a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="#">Separated link</a>
                    </li>
                </ul>
            </div>
            {{> page.buttons_dropdown-buttons}}

            <h3 class="header smaller">
                Grouped Buttons
            </h3>

            <div class="btn-group">
                <button class="btn btn-primary">Save</button>
                <button class="btn btn-primary">Update</button>
                <button class="btn btn-primary">Delete</button>
            </div>
            <div class="btn-group">
                <button class="btn btn-primary">Ok</button>
                <button class="btn btn-default">Cancel</button>
            </div>
            {{> page.buttons_grouped-buttons}}
        </div>

        <div id="progressbar" class="tab-pane">
            <h3 class="header smaller">
                Progress Bar
            </h3>

            <div class="progress progress-small progress-striped active">
