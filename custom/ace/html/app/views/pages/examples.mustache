                <div class="progress-bar progress-bar-striped progress-bar-warning" style="width: 20%"></div>
                <div class="progress-bar progress-bar-striped progress-bar-danger" style="width: 10%"></div>
            </div>
            {{> page.progress-bar_stacked}}
